    <!-- appSettings 
    Whem moving this to another machine, you need to unencrypt , make your changes, then encrypt
    Execute the following:
    Encrypt: ConfigSectionCrypt.exe -e app.config appSettings    
    Decrypt: ConfigSectionCrypt.exe -d app.config appSettings    
    <appSettings>
      <add key="Path" value="C:\Program Files\SIEGate\Archive"/>
      <add key="Logger" value="Logger.ConsoleLogger,Logger.FileLogger" />
      <add key="Minutes" value="5"/>
      <add key="DaysToKeep" value="7"/>
      <add key="AlertLimitMegabyte" value="2048"/>
      <add key="AlertMinutes" value="5"/>
      <add key="MailServer" value=""/>
      <add key="SMTPPort" value="587"/>
      <add key="Username" value=""/>
      <add key="Password" value=""/>
      <add key="MailTo" value=""/>
      <add key="MailFrom" value=""/>
    </appSettings>
    -->
    <appSettings configProtectionProvider="DataProtectionConfigurationProvider">
        <EncryptedData>
            <CipherData>
                <CipherValue></CipherValue>
            </CipherData>
        </EncryptedData>
    </appSettings>

 
  <applicationSettings>
        <SIEGateCleanup.Properties.Settings>
            <setting name="Path" serializeAs="String">
                <value>C:\Program Files\SIEGate\Archive</value>
            </setting>
            <setting name="Minutes" serializeAs="String">
                <value>1</value>
            </setting>
        </SIEGateCleanup.Properties.Settings>
    </applicationSettings>
</configuration>